{
  "name": "nigella",
  "version": "1.0.0",
  "description": "API code base",
  "main": "index.js",
  "scripts": {
    "lint": "npx eslint src/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/renagalaxy/nigella.git"
  },
  "author": "renagalaxy",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "log4js": "^6.1.1",
    "request": "^2.88.0",
    "request-promise": "^4.2.5",
    "swagger-ui-express": "^4.1.3"
  },
  "devDependencies": {
    "eslint": "^6.8.0"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module"
    },
    "rules": {
      "indent": [
        "error",
        4
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "never"
      ]
    }
  }
}
